{
  "name": "my-cool-proxy-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @karashiiro/my-cool-proxy dev",
    "test": "vitest run",
    "test:unit": "vitest run --exclude '**/e2e/**'",
    "test:e2e": "vitest run apps/gateway/src/e2e",
    "test:e2e:http": "vitest run apps/gateway/src/e2e/http",
    "test:e2e:stdio": "vitest run apps/gateway/src/e2e/stdio",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "typecheck": "pnpm -r typecheck",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}
